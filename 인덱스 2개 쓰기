<?php
 //배열 선언
 $earth = array();

 $earth['nation'][0] = 'korea';
 $earth['nation'][1] = 'america';
 $earth['nation'][2] = 'canada';
 $earth['nation'][3] = 'japan';
 $earth['nation'][4] = 'germany';
 $earth['nation'][5] = 'france';
 $earth['nation'][6] = 'china';

//배열의 내용을 다 보여줌
